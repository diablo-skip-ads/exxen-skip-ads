{
  "manifest_version": 3,
  "name": "Adblock for Exxen",
  "version": "0.1",
  "description": "RKLMVAR paketi kullanıcıları için reklamsız Exxen kullanım deneyimi sunar",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["*://*.exxen.com/*"],
  "author": "Diablo",
  "short_name": "Adblock for Exxen",
  "content_scripts": [
    {
      "matches": ["*://*.exxen.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Adblock for Exxen",
    "default_icon": {
      "16": "/images/logo-16.png",
      "32": "/images/logo-32.png",
      "48": "/images/logo-48.png",
      "128": "/images/logo-128.png"
    }
  },
  "icons": {
    "16": "/images/logo-16.png",
    "32": "/images/logo-32.png",
    "48": "/images/logo-48.png",
    "128": "/images/logo-128.png"
  }
}
